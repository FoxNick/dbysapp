<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="ysimg">
        <item name="android:layout_width">130dp</item>
        <item name="android:layout_height">185dp</item>
    </style>
    <style name="Tablayoutstyle" parent="Base.Widget.Design.TabLayout">
        <item name="tabBackground">@color/GhostWhite</item>
        <item name="tabIndicatorColor">@color/SeaGreen1</item>
        <item name="tabIndicatorHeight">2dp</item>
        <item name="tabSelectedTextColor">@color/Orange1</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/Orange1</item>
    </style>
</resources>
